wire flagZ;
wire NOT_ULA;
wire OR_ULA;
wire [7:0] PC_OUT;
wire [7:0] RDM_OUT;
wire [7:0] RI_OUT;
wire [2:0] sel_ALU;
wire Y_ULA;

wire sampler;                             

// assign statements (if any)                          
NEANDER i1 (
// port map - connection between master ports and signals/registers   
	.AC_OUT(AC_OUT),
	.ADD_ULA(ADD_ULA),
	.ALU_OUT(ALU_OUT),
	.AND_ULA(AND_ULA),
