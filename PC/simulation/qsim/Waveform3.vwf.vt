module PC_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clk;
reg [7:0] info;
reg [1:0] OPC;
// wires                                               
wire [7:0] S;
wire [7:0] st;

wire sampler;                             

// assign statements (if any)                          
PC i1 (
// port map - connection between master ports and signals/registers   
	.clk(clk),
	.info(info),
	.OPC(OPC),
	.S(S),
	.st(st)
);

// clk
initial
begin
	repeat(16)
	begin
		clk = 1'b0;
		clk = #30000 1'b1;
		# 30000;
	end
	clk = 1'b0;
	clk = #30000 1'b1;
end 
// info[ 7 ]
initial
begin
	info[7] = 1'b0;
end 
// info[ 6 ]
