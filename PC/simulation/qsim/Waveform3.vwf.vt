`timescale 1 ps/ 1 ps
module PC_vlg_sample_tst(
	clk,
	info,
	OPC,
	sampler_tx
);
input  clk;
input [7:0] info;
input [1:0] OPC;
output sampler_tx;

reg sample;
time current_time;
always @(clk or info or OPC)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

