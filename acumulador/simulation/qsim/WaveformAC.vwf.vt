module AC_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clk;
reg [7:0] IN;
reg Reset;
// wires                                               
wire N;
wire [7:0] OUT;
wire Z;

wire sampler;                             

// assign statements (if any)                          
AC i1 (
// port map - connection between master ports and signals/registers   
	.clk(clk),
	.IN(IN),
	.N(N),
	.OUT(OUT),
	.Reset(Reset),
	.Z(Z)
);

// clk
initial
begin
	repeat(16)
	begin
		clk = 1'b0;
		clk = #30000 1'b1;
		# 30000;
	end
	clk = 1'b0;
	clk = #30000 1'b1;
end 
// IN[ 7 ]
initial
begin
	IN[7] = 1'b0;
end 
// IN[ 6 ]
initial
begin
	IN[6] = 1'b0;
end 
// IN[ 5 ]
initial
begin
	IN[5] = 1'b0;
end 
// IN[ 4 ]
initial
begin
	IN[4] = 1'b0;
	IN[4] = #640000 1'b1;
end 
// IN[ 3 ]
initial
begin
	IN[3] = 1'b0;
	IN[3] = #320000 1'b1;
	IN[3] = #320000 1'b0;
	IN[3] = #320000 1'b1;
end 
// IN[ 2 ]
initial
begin
	repeat(3)
	begin
		IN[2] = 1'b0;
		IN[2] = #160000 1'b1;
		# 160000;
	end
	IN[2] = 1'b0;
end 
// IN[ 1 ]
initial
begin
	repeat(6)
	begin
		IN[1] = 1'b0;
