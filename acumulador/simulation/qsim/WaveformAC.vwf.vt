`timescale 1 ps/ 1 ps
module AC_vlg_sample_tst(
	clk,
	IN,
	Reset,
	sampler_tx
);
input  clk;
input [7:0] IN;
input  Reset;
output sampler_tx;

reg sample;
time current_time;
always @(clk or IN or Reset)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

module AC_vlg_check_tst (
	N,
	OUT,
	Z,
	sampler_rx
);
input  N;
input [7:0] OUT;
input  Z;
input sampler_rx;

reg  N_expected;
reg [7:0] OUT_expected;
reg  Z_expected;

reg  N_prev;
reg [7:0] OUT_prev;
reg  Z_prev;

reg  N_expected_prev;
reg [7:0] OUT_expected_prev;
reg  Z_expected_prev;

reg  last_N_exp;
reg [7:0] last_OUT_exp;
reg  last_Z_exp;

reg trigger;

integer i;
integer nummismatches;

reg [1:3] on_first_change ;


initial
begin
trigger = 0;
i = 0;
nummismatches = 0;
on_first_change = 3'b1;
end

// update real /o prevs

always @(trigger)
begin
	N_prev = N;
	OUT_prev = OUT;
	Z_prev = Z;
end

// update expected /o prevs

always @(trigger)
begin
	N_expected_prev = N_expected;
	OUT_expected_prev = OUT_expected;
	Z_expected_prev = Z_expected;
end



// expected Z
initial
begin
	Z_expected = 1'bX;
end 

// expected N
initial
begin
	N_expected = 1'bX;
end 
// expected OUT[ 7 ]
initial
begin
	OUT_expected[7] = 1'bX;
end 
// expected OUT[ 6 ]
initial
begin
	OUT_expected[6] = 1'bX;
end 
// expected OUT[ 5 ]
